package arreglos;

public class Ejercicio1 {
	
	private int [] v;

	public Ejercicio1(int[] v) {
		
		this.v = v;
	}
     public String mostrarArreglos() {
    	 String s = "";
    	 for (int i = 0; i < v.length; i++) {
			s += v[i] + " ";
		}
    	 return s;
     }
    

	public static void main(String[] args) {
		int [] v = {1,2,3,4,5,6,7,8,9};
		
		

	}

}
